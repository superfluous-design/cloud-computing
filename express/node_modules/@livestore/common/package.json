  "dependencies": {
    "@opentelemetry/api": "1.9.0",
    "graphology": "0.26.0-alpha1",
    "graphology-dag": "0.4.1",
    "graphology-types": "0.24.8",
    "@livestore/webmesh": "0.3.1",
    "@livestore/utils": "0.3.1"
  },
  "devDependencies": {
    "vitest": "^3.1.2",
    "@livestore/utils-dev": "0.3.1"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest"
  }
