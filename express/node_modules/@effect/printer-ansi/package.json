{
  "name": "@effect/printer-ansi",
  "version": "0.43.5",
  "description": "An easy to use, extensible pretty-printer for rendering documents for the terminal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/printer-ansi"
  },
  "sideEffects": [],
  "homepage": "https://effect.website",
  "dependencies": {
    "@effect/printer": "^0.43.5"
  },
  "peerDependencies": {
    "@effect/typeclass": "^0.34.5",
    "effect": "^3.15.5"
  },
  "publishConfig": {
    "provenance": true
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./Ansi": {
      "types": "./dist/dts/Ansi.d.ts",
      "import": "./dist/esm/Ansi.js",
      "default": "./dist/cjs/Ansi.js"
    },
    "./AnsiDoc": {
      "types": "./dist/dts/AnsiDoc.d.ts",
      "import": "./dist/esm/AnsiDoc.js",
      "default": "./dist/cjs/AnsiDoc.js"
    },
    "./Color": {
      "types": "./dist/dts/Color.d.ts",
      "import": "./dist/esm/Color.js",
      "default": "./dist/cjs/Color.js"
    },
    "./index": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "Ansi": [
        "./dist/dts/Ansi.d.ts"
      ],
      "AnsiDoc": [
        "./dist/dts/AnsiDoc.d.ts"
      ],
      "Color": [
        "./dist/dts/Color.d.ts"
      ],
      "index": [
        "./dist/dts/index.d.ts"
      ]
    }
  }
}