{
  "name": "@effect/printer",
  "version": "0.43.5",
  "description": "An easy to use, extensible pretty-printer for rendering documents",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/printer"
  },
  "sideEffects": [],
  "homepage": "https://effect.website",
  "peerDependencies": {
    "@effect/typeclass": "^0.34.5",
    "effect": "^3.15.5"
  },
  "publishConfig": {
    "provenance": true
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./Doc": {
      "types": "./dist/dts/Doc.d.ts",
      "import": "./dist/esm/Doc.js",
      "default": "./dist/cjs/Doc.js"
    },
    "./DocStream": {
      "types": "./dist/dts/DocStream.d.ts",
      "import": "./dist/esm/DocStream.js",
      "default": "./dist/cjs/DocStream.js"
    },
    "./DocTree": {
      "types": "./dist/dts/DocTree.d.ts",
      "import": "./dist/esm/DocTree.js",
      "default": "./dist/cjs/DocTree.js"
    },
    "./Flatten": {
      "types": "./dist/dts/Flatten.d.ts",
      "import": "./dist/esm/Flatten.js",
      "default": "./dist/cjs/Flatten.js"
    },
    "./Layout": {
      "types": "./dist/dts/Layout.d.ts",
      "import": "./dist/esm/Layout.js",
      "default": "./dist/cjs/Layout.js"
    },
    "./Optimize": {
      "types": "./dist/dts/Optimize.d.ts",
      "import": "./dist/esm/Optimize.js",
      "default": "./dist/cjs/Optimize.js"
    },
    "./PageWidth": {
      "types": "./dist/dts/PageWidth.d.ts",
      "import": "./dist/esm/PageWidth.js",
      "default": "./dist/cjs/PageWidth.js"
    },
    "./index": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "Doc": [
        "./dist/dts/Doc.d.ts"
      ],
      "DocStream": [
        "./dist/dts/DocStream.d.ts"
      ],
      "DocTree": [
        "./dist/dts/DocTree.d.ts"
      ],
      "Flatten": [
        "./dist/dts/Flatten.d.ts"
      ],
      "Layout": [
        "./dist/dts/Layout.d.ts"
      ],
      "Optimize": [
        "./dist/dts/Optimize.d.ts"
      ],
      "PageWidth": [
        "./dist/dts/PageWidth.d.ts"
      ],
      "index": [
        "./dist/dts/index.d.ts"
      ]
    }
  }
}